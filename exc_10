fun main() {
    val list = arrayOf(2,3,4,5,6)
    var i = 0
    var w = 0
    
    while (i < list.size) {
        w = if (i%2 == 1) 2 else 5
        i++
    }
    
    print(
        when (w) {
            1 -> "um"
            2 -> "dois"
            3 -> "trÃªs"
            5 -> "cinco"
            else -> "outro"
        }
    )
}
